  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.9",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "sucrase": "^3.16.0",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "eslint": "^7.11.0",
    "nodemon": "^2.0.6"
  }
